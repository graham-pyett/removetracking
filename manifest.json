{
  "manifest_version": 3,
  "name": "Remove Tracking",
  "description": "Get rid of trackers from URLs",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/favicon-32x32.png"
  },
  "content_scripts": [
    {
      "matches": ["https://tiktok.com/*", "https://www.tiktok.com/*", "http://tiktok.com/*", "http://www.tiktok.com/*"],
      "js": ["content.js"]
    }
  ]
}